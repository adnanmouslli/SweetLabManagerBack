generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  username  String   @unique
  password  String
  roles     Role[]   @default([EMPLOYEE])
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

enum Role {
  MANAGER
  ADMIN
  EMPLOYEE
}
